{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 4,
  "final_space": true,
  "console_title_template": "{{.Folder}}",
  "palette": {
    "error": "#d16969",
    "success": "#8db07b",
    "console:foreground": "#d4d4d4",
    "console:bright_foreground": "#ffffff",
    "console:background": "#1e1e1e",
    "cyan": "#569cd6",
    "half_cyan": "#284e6b",
    "yellow": "#d7ba7d",
    "bright_purple": "#c586c0",
    "git:ahead": "p:success",
    "git:behind": "p:error",
    "git:ahead_and_behind": "p:yellow"
  },
  "valid_line": {
    "foreground": "p:success",
    "background": "transparent",
    "template": " "
  },
  "error_line": {
    "foreground": "p:error",
    "background": "transparent",
    "template": "\uf12a"
  },
  "secondary_prompt": {
    "template": "  \ue0b1 "
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "status",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:console:bright_foreground",
          "background": "p:success",
          "background_templates": ["{{ if gt .Code 0 }}p:error{{ end }}"],
          "template": " {{ if gt .Code 0 }}\uf467{{ else }}\uf42e{{ end }} ",
          "options": {
            "always_enabled": true
          }
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:console:background",
          "background": "p:console:foreground",
          "template": " {{ .FormattedMs }} ",
          "options": {
            "always_enabled": true,
            "style": "austin"
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:console:foreground",
          "background": "transparent",
          "template": " {{ .CurrentDate | date .Format }} "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:console:background",
          "template": "anchor123"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "upgrade",
          "style": "plain",
          "foreground": "p:yellow",
          "background": "transparent",
          "template": "Oh My Posh upgrade available ({{ .Current }} -> {{ .Latest }})"
        }
      ]
    },
    {
      "newline": true,
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "aws",
          "style": "plain",
          "foreground": "p:console:foreground",
          "template": " <#fb9e2d>\ue7ad</> {{.Profile}}",
          "templateComment__\ue7ad": ""
        },
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "p:console:foreground",
          "background": "transparent",
          "template": " <#8931ff>\ue77f</>{{ if .Full }} {{ .Full }}{{ end }}",
          "options": {
            "fetch_version": false
          }
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:console:foreground",
          "background": "transparent",
          "template": " {{ .PackageManagerIcon }} <#5fa04e>\ue718</> {{ if .Full }}{{ .Full }}{{ end }}",
          "options": {
            "fetch_version": true,
            "fetch_package_manager": true,
            "yarn_icon": "<#2c8ebb>\ue6a7</>",
            "npm_icon": "<#ff4300>\ue71e</>",
            "comment_yarn_icon": "",
            "comment_npm_icon": ""
          }
        },
        {
          "type": "angular",
          "style": "plain",
          "foreground": "p:console:foreground",
          "background": "transparent",
          "template": " <#ff4300>\ue753</> {{ if .Full }}{{ .Full }}{{ end }}",
          "options": {
            "fetch_version": true
          }
        },
        {
          "alias": "FileTypes",
          "type": "text",
          "style": "plain",
          "foreground": "p:console:foreground",
          "background": "transparent",
          "templates": [
            "{{ .Env.omp_files_readme }}{{ .Env.omp_files_dockercompose }}"
          ]
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:console:foreground",
          "background": "transparent",
          "foreground_templates": [
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git:ahead_and_behind{{ end }}",
            "{{ if gt .Ahead 0 }}p:git:ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git:behind{{ end }}"
          ],
          "template": " {{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}<p:git:behind> {{ .Working.String }}</>{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }}{{ end }}{{ if .Staging.Changed }}<p:git:ahead> {{ .Staging.String }}</>{{ end }}",
          "options": {
            "fetch_status": false,
            "source": "pwsh",
            "branch_icon": "<p:yellow>\udb81\ude2c</> ",
            "branch_identical_icon": "<p:cyan>\u2261</>",
            "branch_ahead_icon": "<p:git:ahead>\u2191</>",
            "branch_behind_icon": "<p:git:behind>\u2193</>",
            "branch_gone_icon": "<p:bright_purple>\u2262</>",
            "commit_icon": "\uf417 ",
            "tag_icon": "\uf412 ",
            "templateComment__branch_icon": "f062c/󰘬",
            "templateComment__branch_identical_icon": "≡",
            "templateComment__branch_ahead_icon": "↑",
            "templateComment__branch_behind_icon": "↓",
            "templateComment__branch_gone_icon": "≢",
            "templateComment__commit_icon": " ",
            "templateComment__tag_icon": " ",
            "templateComment__.HEAD__normal": "󰘬master",
            "templateComment__.HEAD__rebase": " 󰘬branch onto 󰘬master (1/3) at  abc1234",
            "templateComment__.BranchStatus": "≡",
            "templateComment__.Working.String": "+2 ~6",
            "templateComment__.Staging.String": "~1"
          }
        }
      ]
    },
    {
      "newline": true,
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:console:bright_foreground",
          "background": "p:half_cyan",
          "templates": [
            "{{ if regexMatch (replace \"/\" \"\\\\\\\\\" \"C:/BenLocal/git(/.+)?$\") .Path }} git {{ end }}",
            "{{ if regexMatch (replace \"/\" \"\\\\\\\\\" \"C:/Work/repos(/.+)?$\") .Path }} {{- if .Env.WorkReposLabel }} {{ .Env.WorkReposLabel }} {{ else }} repos {{ end -}} {{ end }}",
            "{{ if regexMatch (replace \"/\" \"\\\\\\\\\" \"C:/Work/repos-worktrees(/.+)?$\") .Path }} {{- if .Env.WorkReposLabel }} {{ .Env.WorkReposLabel }}-worktrees {{ else }} worktrees {{ end -}} {{ end }}",
            "{{ if regexMatch (replace \"/\" \"\\\\\\\\\" \"~/OneDrive(/.+)?$\") .Path }} OneDrive {{ end }}"
          ],
          "templates_logic": "first_match",
          "include_folders": [
            "C:/BenLocal/git(/.*)?",
            "C:/Work/repos(/.*)?",
            "C:/Work/repos-worktrees(/.*)?",
            "~/OneDrive(/.*)?"
          ],
          "options": {
            "style": "full"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:console:bright_foreground",
          "background": "p:cyan",
          "template": " {{ regexReplaceAll \"^\\\\\\\\(.+)\" .Path \"$1\" }} ",
          "options": {
            "style": "full",
            "mapped_locations": {
              "C:/BenLocal/git": "\\",
              "C:/Work/repos": "\\",
              "C:/Work/repos-worktrees": "\\",
              "{{ .Env.OneDrive }}": "\\"
            }
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "p:console:background",
          "background": "p:yellow",
          "template": " {{ if .StackCount }}{{ .StackCount }}{{ end }} "
        }
      ]
    }
  ]
}
